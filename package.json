{
    "name":        "upd",
    "homepage":    "http://github.com/rse/upd",
    "description": "Upgrade NPM Package Dependencies",
    "version":     "2.8.3",
    "license":     "MIT",
    "author": {
        "name":    "Dr. Ralf S. Engelschall",
        "email":   "rse@engelschall.com",
        "url":     "http://engelschall.com"
    },
    "keywords": [
        "upgrade", "package", "dependency", "npm", "package.json"
    ],
    "repository": {
        "type": "git",
        "url":  "git://github.com/rse/upd.git"
    },
    "bugs": {
        "url":  "http://github.com/rse/upd/issues"
    },
    "bin": {
        "upd":  "./upd.js"
    },
    "devDependencies": {
        "eslint":                 "8.12.0",
        "eslint-config-standard": "17.0.0-1",
        "eslint-plugin-promise":  "6.0.0",
        "eslint-plugin-import":   "2.25.4",
        "eslint-plugin-node":     "11.1.0"
    },
    "dependencies" : {
        "yargs":                  "17.4.0",
        "semver":                 "7.3.5",
        "awaity":                 "1.0.0",
        "pacote":                 "13.0.5",
        "progress":               "2.0.3",
        "event-stream":           "4.0.1",
        "stream-wrapper":         "0.1.2",
        "pretty-bytes":           "5.6.0",
        "update-notifier":        "5.1.0",
        "fast-diff":              "1.2.0",
        "cli-table3":             "0.6.1",
        "ducky":                  "2.7.3",
        "escape-string-regexp":   "4.0.0",
        "chalk":                  "4.1.2",
        "strip-ansi":             "6.0.0",
        "micromatch":             "4.0.5",
        "json-asty":              "1.0.5"
    },
    "upd": [
        "!escape-string-regexp",
        "!strip-ansi"
    ],
    "engines": {
        "node":                   ">=12.0.0"
    },
    "scripts": {
        "prepublishOnly":         "eslint --config eslint.yaml upd.js"
    }
